{
  "schemaVersion": 1,
  "id": "meteor-translator",
  "version": "${version}",
  "name": "MeteorTranslator",
  "description": "Unofficial translations for Meteor",
  "authors": [
    "GoJean"
  ],
  "contact": {
    "sources": "https://github.com/GoJean/MeteorTranslator"
  },
  "icon": "assets/meteor-translator/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "name.gojean.meteor.translator.MeteorTranslator"
    ]
  },
  "mixins": [
    "addon-translator.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25",
    "modmenu": {
      "parent": {
        "id": "meteor-client"
      }
    }
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
